socket.onAny((event, args1) -> {
    System.out.println("📡 Evento recibido: " + event);
    if (args1.length > 0) {
        System.out.println(args1[0]);
    }
});



8************


socket.emit("send_order", payload, new Ack() {
    @Override
    public void call(Object... args) {
        System.out.println("📩 ACK recibido:");
        for (Object arg : args) {
            System.out.println(arg);
        }
    }
});



errores ************

socket.on(Socket.EVENT_CONNECT_ERROR, args1 ->
    System.out.println("❌ Error de conexión: " + args1[0]));

socket.on(Socket.EVENT_ERROR, args1 ->
    System.out.println("❌ Error: " + args1[0]));
